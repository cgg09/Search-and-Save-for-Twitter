<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tfg.view.LoginViewController">
   <children>
      <GridPane layoutX="72.0" layoutY="95.0" prefHeight="60.0" prefWidth="259.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="username" GridPane.columnIndex="1" />
            <Label text="Usuario:" />
            <Label text="Contraseña:" GridPane.rowIndex="1" />
            <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="161.0" layoutY="175.0" mnemonicParsing="false" onAction="#handleLogin" text="Inicia sesión" />
      <Label layoutX="110.0" layoutY="43.0" text="Inicia sesión en Twitter">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Hyperlink fx:id="link" layoutX="116.0" layoutY="212.0" onAction="#handleLink" text="¿No tienes cuenta? Regístrate." />
   </children>
</AnchorPane>
